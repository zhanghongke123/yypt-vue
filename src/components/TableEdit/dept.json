          { "field": "deptId", "title": "部门ID:", "span": 6, "itemRender": { "name": "$input", "props": { "placeholder": "" } } },
          { "field": "deptName", "title": "部门名称:", "span": 6, "itemRender": { "name": "$input", "props": { "placeholder": "" } } },
          { "span": 6, "align": "center", "collapseNode": false, "itemRender": { "name": "$buttons", "children": [{ "props": { "type": "submit", "content": "查询", "status": "primary"} }, { "props": { "type": "reset", "content": "重置" } }] } }
